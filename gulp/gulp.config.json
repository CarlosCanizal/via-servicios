{
  "client": "src/client/",
  "htmlTemplates": "src/client/app/**/*.html",
  "jsonTemplates": "src/client/app/**/*.json",
  "vendorJs": [
    "src/client/bower_components/angular/angular.min.js",
    "src/client/bower_components/angular-ui-router/release/angular-ui-router.min.js",
    "src/client/bower_components/angular-animate/angular-animate.min.js",
    "src/client/bower_components/jquery/dist/jquery.min.js",
    "src/client/bower_components/angular-ui-router-anim-in-out/anim-in-out.js",
    "src/client/bower_components/lodash/dist/lodash.min.js",
    "src/client/bower_components/restangular/dist/restangular.min.js"
  ],
  "vendorCss": [
    "src/client/bower_components/bootstrap/dist/css/bootstrap.min.css",
    "src/client/bower_components/font-awesome/css/font-awesome.min.css",
    "src/client/bower_components/angular-ui-router-anim-in-out/css/anim-in-out.css"
